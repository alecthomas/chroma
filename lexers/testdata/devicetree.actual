/dts-v1/;

/* Bootloader installs ATF here */
/memreserve/ 0x80000000 0x200000;

#include <dt-bindings/leds/common.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include "en7523.dtsi"

/ {
	model = "TP-Link xx230v";
	compatible = "tplink,xx230v", "airoha,en7523";

	chosen {
		bootargs = "console=ttyS0,115200 earlycon";
		stdout-path = "serial0:115200n8";
	};

	aliases {
		serial0 = &uart1;
		led-boot = &led_power;
		led-failsafe = &led_power;
		led-upgrade = &led_power;
		led-running = &led_power;
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x80000000 0x40000000>;
	};

	keys {
		// compatible = "gpio-keys";
		compatible = "gpio-keys-polled";
		poll-interval = <100>;

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio0 0 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		wps {
			label = "wps";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio0 7 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};

	leds {
		compatible = "gpio-leds";

		// Power
		led_power: led@0 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_STATUS;
			function-enumerator = <0>;
			gpios = <&gpio0 11 GPIO_ACTIVE_LOW>;
		};

		// GPON
		led@1 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_STATUS;
			function-enumerator = <1>;
			gpios = <&gpio0 10 GPIO_ACTIVE_LOW>;
		};

		// LOS
		led@2 {
			color = <LED_COLOR_ID_RED>;
			function = LED_FUNCTION_STATUS;
			function-enumerator = <2>;
			gpios = <&gpio0 3 GPIO_ACTIVE_HIGH>;
		};

		// internet
		led@3 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_STATUS;
			function-enumerator = <3>;
			gpios = <&gpio0 1 GPIO_ACTIVE_LOW>;
		};

		// Wireless
		led@4 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WLAN;
			function-enumerator = <0>;
			gpios = <&gpio0 6 GPIO_ACTIVE_LOW>;
		};

		// Phone
		led@5 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_STATUS;
			function-enumerator = <4>;
			gpios = <&gpio0 5 GPIO_ACTIVE_HIGH>;
		};

		led@6 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_LAN;
			function-enumerator = <0>;
			gpios = <&gpio0 22 GPIO_ACTIVE_LOW>;
		};

		led@7 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_LAN;
			function-enumerator = <1>;
			gpios = <&gpio0 23 GPIO_ACTIVE_LOW>;
		};

		led@8 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_LAN;
			function-enumerator = <2>;
			gpios = <&gpio0 24 GPIO_ACTIVE_LOW>;
		};

		led@9 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WAN;
			function-enumerator = <0>;
			gpios = <&gpio0 25 GPIO_ACTIVE_LOW>;
		};
	};
};

&nand {
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		partition@0 {
			label = "bootloader";
			reg = <0x00000 0x80000>;
			read-only;
		};

		// filled with 0xff
		partition@80000 {
			label = "romfile";
			reg = <0x80000 0xa0000>;
			read-only;
		};

		partition@a0000 {
			label = "misc";
			reg = <0x0a0000 0x1a0000>;
			read-only;

			nvmem-layout {
				compatible = "fixed-layout";
				#address-cells = <1>;
				#size-cells = <1>;

				switch_macaddr: macaddr@4f100 {
					#nvmem-cell-cells = <1>;
					compatible = "mac-base";
					reg = <0x4f100 0x6>;
				};

				wifi_macaddr: macaddr@4f1e0 {
					#nvmem-cell-cells = <1>;
					compatible = "mac-base";
					reg = <0x4f1e0 0x6>;
				};

				gpon_serial_number: gpon@4c000 {
					#nvmem-cell-cells = <1>;
					reg = <0x4c000 0x3f00>;
				};

				eeprom0: eeprom@80000 {
					reg = <0x80000 0xe00>;
				};
			};
		};

		partition@1a0000 {
			label = "kernel";
			reg = <0x01a0000 0x1000000>;
		};

		partition@11a0000 {
			label = "rootfs";
			reg = <0x11a0000 0x4e60000>;
		};

		/* reserved for bad block table */
		reserved_bmt@3fe0000 {
			label = "reserved_bmt";
			reg = <0x3fe0000 0x4000000>;
			read-only;
		};
	};
};

&pcie0 {
	status = "okay";
};

&pcie1 {
	status = "okay";

	pcie@1,0 {
		reg = <0x00000800 0 0 0 0>;
		#address-cells = <3>;
		#size-cells = <2>;

		mt7915@0,0 {
			reg = <0x0000 0 0 0 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			nvmem-cells = <&eeprom0>;
			nvmem-cell-names = "eeprom";
			airoha,npu = <&npu>;
			airoha,eth = <&eth>;

			// 2.4 GHz
			band@0 {
				reg = <0>;
				nvmem-cells = <&wifi_macaddr 0>;
				nvmem-cell-names = "mac-address";
			};

			// 5 GHz
			band@1 {
				reg = <1>;
				nvmem-cells = <&wifi_macaddr 2>;
				nvmem-cell-names = "mac-address";
			};
		};
	};
};

&usb0 {
	status = "okay";
};

&i2c0 {
	status = "okay";
};

&npu {
	status = "okay";
};

&eth {
	status = "okay";
};

&gdm1 {
	status = "okay";

	nvmem-cells = <&switch_macaddr 0>;
	nvmem-cell-names = "mac-address";
};

&switch {
	status = "okay";
	ports {
		port@1 {
			label = "lan1";
			status = "okay";
		};

		port@2 {
			label = "lan2";
			status = "okay";
		};

		port@3 {
			label = "lan3";
			status = "okay";
		};

		port@4 {
			label = "wan";
			status = "okay";
		};

		port@6 {
			nvmem-cells = <&switch_macaddr 0>;
			nvmem-cell-names = "mac-address";
		};
	};
};
